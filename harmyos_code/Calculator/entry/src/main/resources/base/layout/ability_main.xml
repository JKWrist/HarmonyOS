<?xml version="1.0" encoding="utf-8"?>
<DependentLayout
    xmlns:ohos="http://schemas.huawei.com/res/ohos"
    ohos:height="match_parent"
    ohos:width="match_parent"
    ohos:background_element="$graphic:background_ability_main"
    ohos:orientation="vertical">

    <DependentLayout
        xmlns:ohos="http://schemas.huawei.com/res/ohos"
        ohos:id="$+id:dep1"
        ohos:height="200vp"
        ohos:width="match_parent"
        ohos:orientation="vertical">

        <TextField
            ohos:id="$+id:textfield"
            ohos:height="100vp"

            ohos:width="match_parent"
            ohos:padding="10vp"
            ohos:text_color="#000000"
            ohos:text_size="30fp"
            ohos:text_weight="1000"></TextField>

        <Text
            ohos:id="$+id:text_res"
            ohos:height="100vp"

            ohos:width="match_parent"
            ohos:below="$+id:textfield"
            ohos:padding="10vp"
            ohos:text_color="#000000"
            ohos:text_size="30fp"
            ohos:text_weight="1000"></Text>
        >
    </DependentLayout>

    <DependentLayout
        xmlns:ohos="http://schemas.huawei.com/res/ohos"
        ohos:id="$+id:dep2"
        ohos:height="match_parent"
        ohos:width="match_parent"
        ohos:background_element="$graphic:buts_all"
        ohos:below="$+id:dep1"
        ohos:left_margin="10vp"
        ohos:orientation="vertical"
        ohos:right_margin="10vp"
        ohos:top_margin="20"
        >

        <Button
            ohos:id="$+id:btC"
            ohos:height="75vp"
            ohos:width="75vp"
            ohos:background_element="$graphic:btn"
            ohos:bottom_margin="5vp"
            ohos:left_margin="5vp"
            ohos:right_margin="5vp"
            ohos:text="C"
            ohos:text_alignment="center"
            ohos:text_color="#0F5DF5"
            ohos:text_size="30fp"
            ohos:text_weight="1000"
            ohos:top_margin="5vp"
            />

        <Button
            ohos:id="$+id:bt_div"
            ohos:height="75vp"
            ohos:width="75vp"
            ohos:background_element="$graphic:btn"
            ohos:bottom_margin="5vp"
            ohos:left_margin="5vp"
            ohos:right_margin="5vp"
            ohos:right_of="$+id:btC"
            ohos:text="÷"
            ohos:text_alignment="center"
            ohos:text_color="#0F5DF5"
            ohos:text_size="30fp"
            ohos:text_weight="1000"
            ohos:top_margin="5vp"
            />

        <Button
            ohos:id="$+id:bt_mul"
            ohos:height="75vp"
            ohos:width="75vp"
            ohos:background_element="$graphic:btn"
            ohos:bottom_margin="5vp"
            ohos:left_margin="5vp"
            ohos:right_margin="5vp"
            ohos:right_of="$+id:bt_div"
            ohos:text="×"
            ohos:text_alignment="center"
            ohos:text_color="#0F5DF5"
            ohos:text_size="30fp"
            ohos:text_weight="1000"
            ohos:top_margin="5vp"
            />

        <Button
            ohos:id="$+id:bt_delete"
            ohos:height="75vp"
            ohos:width="75vp"
            ohos:background_element="$graphic:btn"
            ohos:bottom_margin="5vp"
            ohos:left_margin="5vp"
            ohos:right_margin="5vp"
            ohos:right_of="$id:bt_mul"
            ohos:text="←"
            ohos:text_alignment="center"
            ohos:text_color="#0F5DF5"
            ohos:text_font="serif"
            ohos:text_size="40fp"
            ohos:text_weight="1000"
            ohos:top_margin="5vp"/>

        <Button
            ohos:id="$+id:bt7"
            ohos:height="75vp"
            ohos:width="75vp"
            ohos:background_element="$graphic:btn"
            ohos:below="$+id:btC"
            ohos:bottom_margin="5vp"
            ohos:left_margin="5vp"
            ohos:right_margin="5vp"
            ohos:text="7"
            ohos:text_alignment="center"
            ohos:text_color="#171717"
            ohos:text_size="30fp"
            ohos:text_weight="1000"
            ohos:top_margin="5vp"
            />

        <Button
            ohos:id="$+id:bt8"
            ohos:height="75vp"
            ohos:width="75vp"
            ohos:background_element="$graphic:btn"
            ohos:below="$+id:btC"
            ohos:bottom_margin="5vp"
            ohos:left_margin="5vp"
            ohos:right_margin="5vp"
            ohos:right_of="$+id:bt7"
            ohos:text="8"
            ohos:text_alignment="center"
            ohos:text_color="#171717"
            ohos:text_size="30fp"
            ohos:text_weight="1000"
            ohos:top_margin="5vp"
            />

        <Button
            ohos:id="$+id:bt9"
            ohos:height="75vp"
            ohos:width="75vp"
            ohos:background_element="$graphic:btn"
            ohos:below="$+id:btC"
            ohos:bottom_margin="5vp"
            ohos:left_margin="5vp"
            ohos:right_margin="5vp"
            ohos:right_of="$+id:bt8"
            ohos:text="9"
            ohos:text_alignment="center"
            ohos:text_color="#171717"
            ohos:text_size="30fp"
            ohos:text_weight="1000"
            ohos:top_margin="5vp"/>

        <Button
            ohos:id="$+id:bt_de"
            ohos:height="75vp"
            ohos:width="75vp"
            ohos:background_element="$graphic:btn"
            ohos:below="$+id:btC"
            ohos:bottom_margin="5vp"
            ohos:left_margin="5vp"
            ohos:right_margin="5vp"
            ohos:right_of="$id:bt9"
            ohos:text="-"
            ohos:text_alignment="center"
            ohos:text_color="#0F5DF5"
            ohos:text_font="serif"
            ohos:text_size="40fp"
            ohos:top_margin="5vp"/>

        <Button
            ohos:id="$+id:bt4"
            ohos:height="75vp"
            ohos:width="75vp"
            ohos:background_element="$graphic:btn"
            ohos:below="$+id:bt7"
            ohos:bottom_margin="5vp"
            ohos:left_margin="5vp"
            ohos:right_margin="5vp"
            ohos:text="4"
            ohos:text_alignment="center"
            ohos:text_color="#171717"
            ohos:text_size="30fp"
            ohos:text_weight="1000"
            ohos:top_margin="5vp"
            />

        <Button
            ohos:id="$+id:bt5"
            ohos:height="75vp"
            ohos:width="75vp"
            ohos:background_element="$graphic:btn"
            ohos:below="$+id:bt7"
            ohos:bottom_margin="5vp"
            ohos:left_margin="5vp"
            ohos:right_margin="5vp"
            ohos:right_of="$+id:bt4"
            ohos:text="5"
            ohos:text_alignment="center"
            ohos:text_color="#171717"
            ohos:text_size="30fp"
            ohos:text_weight="1000"
            ohos:top_margin="5vp"
            />

        <Button
            ohos:id="$+id:bt6"
            ohos:height="75vp"
            ohos:width="75vp"
            ohos:background_element="$graphic:btn"
            ohos:below="$+id:bt7"
            ohos:bottom_margin="5vp"
            ohos:left_margin="5vp"
            ohos:right_margin="5vp"
            ohos:right_of="$+id:bt5"
            ohos:text="6"
            ohos:text_alignment="center"
            ohos:text_color="#171717"
            ohos:text_size="30fp"
            ohos:text_weight="1000"
            ohos:top_margin="5vp"
            />

        <Button
            ohos:id="$+id:bt_in"
            ohos:height="75vp"
            ohos:width="75vp"
            ohos:background_element="$graphic:btn"
            ohos:below="$+id:bt7"
            ohos:bottom_margin="5vp"
            ohos:left_margin="5vp"
            ohos:right_margin="5vp"
            ohos:right_of="$id:bt9"
            ohos:text="+"
            ohos:text_alignment="center"
            ohos:text_color="#0F5DF5"
            ohos:text_font="serif"
            ohos:text_size="40fp"
            ohos:top_margin="5vp"/>

        <Button
            ohos:id="$+id:bt1"
            ohos:height="75vp"
            ohos:width="75vp"
            ohos:background_element="$graphic:btn"
            ohos:below="$+id:bt4"
            ohos:bottom_margin="5vp"
            ohos:left_margin="5vp"
            ohos:right_margin="5vp"
            ohos:text="1"
            ohos:text_alignment="center"
            ohos:text_color="#171717"
            ohos:text_size="30fp"
            ohos:text_weight="1000"
            ohos:top_margin="5vp"
            />

        <Button
            ohos:id="$+id:bt2"
            ohos:height="75vp"
            ohos:width="75vp"
            ohos:background_element="$graphic:btn"
            ohos:below="$+id:bt4"
            ohos:bottom_margin="5vp"
            ohos:left_margin="5vp"
            ohos:right_margin="5vp"
            ohos:right_of="$+id:bt7"
            ohos:text="2"
            ohos:text_alignment="center"
            ohos:text_color="#171717"
            ohos:text_size="30fp"
            ohos:text_weight="1000"
            ohos:top_margin="5vp"
            />

        <Button
            ohos:id="$+id:bt3"
            ohos:height="75vp"
            ohos:width="75vp"
            ohos:background_element="$graphic:btn"
            ohos:below="$+id:bt4"
            ohos:bottom_margin="5vp"
            ohos:left_margin="5vp"
            ohos:right_margin="5vp"
            ohos:right_of="$+id:bt8"
            ohos:text="3"
            ohos:text_alignment="center"
            ohos:text_color="#171717"
            ohos:text_size="30fp"
            ohos:text_weight="1000"
            ohos:top_margin="5vp"
            />

        <Button
            ohos:id="$+id:bt_eq"
            ohos:height="160vp"
            ohos:width="75vp"
            ohos:background_element="$graphic:ep_background"
            ohos:below="$+id:bt4"
            ohos:bottom_margin="5vp"
            ohos:left_margin="5vp"
            ohos:right_margin="5vp"
            ohos:right_of="$id:bt9"
            ohos:text="="
            ohos:text_alignment="center"
            ohos:text_color="#FFFFFF"
            ohos:text_font="serif"
            ohos:text_size="40fp"
            ohos:top_margin="5vp"/>

        <Button
            ohos:id="$+id:bt_per"
            ohos:height="75vp"
            ohos:width="75vp"
            ohos:background_element="$graphic:btn"
            ohos:below="$+id:bt1"
            ohos:bottom_margin="5vp"
            ohos:left_margin="5vp"
            ohos:right_margin="5vp"
            ohos:text="%"
            ohos:text_alignment="center"
            ohos:text_color="#171717"
            ohos:text_size="30fp"
            ohos:text_weight="1000"
            ohos:top_margin="5vp"
            />

        <Button
            ohos:id="$+id:bt0"
            ohos:height="75vp"
            ohos:width="75vp"
            ohos:background_element="$graphic:btn"
            ohos:below="$+id:bt1"
            ohos:bottom_margin="5vp"
            ohos:left_margin="5vp"
            ohos:right_margin="5vp"
            ohos:right_of="$+id:bt_per"
            ohos:text="0"
            ohos:text_alignment="center"
            ohos:text_color="#171717"
            ohos:text_size="30fp"
            ohos:text_weight="1000"
            ohos:top_margin="5vp"
            />

        <Button
            ohos:id="$+id:bt_end"
            ohos:height="75vp"
            ohos:width="75vp"
            ohos:background_element="$graphic:btn"
            ohos:below="$+id:bt1"
            ohos:bottom_margin="5vp"
            ohos:left_margin="5vp"
            ohos:right_margin="5vp"
            ohos:right_of="$+id:bt0"
            ohos:text="."
            ohos:text_alignment="center"
            ohos:text_color="#171717"
            ohos:text_size="30fp"
            ohos:text_weight="1000"
            ohos:top_margin="5vp"/>

    </DependentLayout>

</DependentLayout>