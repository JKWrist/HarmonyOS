<?xml version="1.0" encoding="utf-8"?>
<DependentLayout
    xmlns:ohos="http://schemas.huawei.com/res/ohos"
    ohos:height="match_parent"
    ohos:width="match_parent"
    ohos:orientation="vertical">

    <DependentLayout
        xmlns:ohos="http://schemas.huawei.com/res/ohos"
        ohos:height="200vp"
        ohos:width="match_parent"
        ohos:orientation="vertical"
        ohos:id="$+id:dep1">

        <TextField
            ohos:id="$+id:textfield"
            ohos:padding="10vp"
            ohos:background_element="$graphic:wite"
            ohos:height="100vp"
            ohos:width="match_parent"
            ohos:text_size="30fp"
            ohos:text_weight="1000"
            ohos:text_color="#000000"></TextField>

        <Text
            ohos:id="$+id:text_res"
            ohos:padding="10vp"
            ohos:background_element="$graphic:wite"
            ohos:height="100vp"
            ohos:width="match_parent"
            ohos:text_size="30fp"
            ohos:text_weight="1000"
            ohos:text_color="#000000"
            ohos:below="$+id:textfield"></Text>
        >
    </DependentLayout>

    <DependentLayout
        xmlns:ohos="http://schemas.huawei.com/res/ohos"
        ohos:height="match_content"
        ohos:width="match_parent"
        ohos:orientation="vertical"
        ohos:below="$+id:dep1"
        ohos:id="$+id:dep2"
        ohos:background_element="$graphic:background_ability_main">

        <Button
            ohos:id="$+id:btC"
            ohos:width="60vp"
            ohos:height="60vp"
            ohos:text_size="25fp"
            ohos:top_margin="15vp"
            ohos:left_margin="15vp"
            ohos:right_margin="15vp"
            ohos:bottom_margin="15vp"
            ohos:text_alignment="center"
            ohos:background_element="$graphic:btn"
            ohos:text="C"
            ohos:text_weight="1000"
            />

        <Button
            ohos:id="$+id:bt_div"
            ohos:width="60vp"
            ohos:height="60vp"
            ohos:text_size="25fp"
            ohos:top_margin="15vp"
            ohos:left_margin="15vp"
            ohos:right_margin="15vp"
            ohos:bottom_margin="15vp"
            ohos:text_alignment="center"
            ohos:background_element="$graphic:btn"
            ohos:text="÷"
            ohos:text_weight="1000"
            ohos:right_of="$+id:btC"
            />

        <Button
            ohos:id="$+id:bt_mul"
            ohos:width="60vp"
            ohos:height="60vp"
            ohos:text_size="25fp"
            ohos:top_margin="15vp"
            ohos:left_margin="15vp"
            ohos:right_margin="15vp"
            ohos:bottom_margin="15vp"
            ohos:text_alignment="center"
            ohos:background_element="$graphic:btn"
            ohos:text="×"
            ohos:text_weight="1000"
            ohos:right_of="$+id:bt_div"
            />

        <Button
            ohos:id="$+id:bt_delete"
            ohos:width="60vp"
            ohos:height="60vp"
            ohos:text_size="25fp"
            ohos:top_margin="15vp"
            ohos:left_margin="15vp"
            ohos:right_margin="15vp"
            ohos:bottom_margin="15vp"
            ohos:text_alignment="center"
            ohos:background_element="$graphic:btn"
            ohos:text="←"
            ohos:text_weight="1000"
            ohos:text_font="serif"
            ohos:right_of="$id:bt_mul"/>

        <Button
            ohos:id="$+id:bt7"
            ohos:width="60vp"
            ohos:height="60vp"
            ohos:text_size="25fp"
            ohos:top_margin="15vp"
            ohos:left_margin="15vp"
            ohos:right_margin="15vp"
            ohos:bottom_margin="15vp"
            ohos:text_alignment="center"
            ohos:background_element="$graphic:btn"
            ohos:text="7"
            ohos:text_weight="1000"
            ohos:below="$+id:btC"
            />

        <Button
            ohos:id="$+id:bt8"
            ohos:width="60vp"
            ohos:height="60vp"
            ohos:text_size="25fp"
            ohos:top_margin="15vp"
            ohos:left_margin="15vp"
            ohos:right_margin="15vp"
            ohos:bottom_margin="15vp"
            ohos:text_alignment="center"
            ohos:background_element="$graphic:btn"
            ohos:text="8"
            ohos:text_weight="1000"
            ohos:right_of="$+id:bt7"
            ohos:below="$+id:btC"
            />

        <Button
            ohos:id="$+id:bt9"
            ohos:width="60vp"
            ohos:height="60vp"
            ohos:text_size="25fp"
            ohos:top_margin="15vp"
            ohos:left_margin="15vp"
            ohos:right_margin="15vp"
            ohos:bottom_margin="15vp"
            ohos:text_alignment="center"
            ohos:background_element="$graphic:btn"
            ohos:text="9"
            ohos:text_weight="1000"
            ohos:right_of="$+id:bt8"
            ohos:below="$+id:btC"/>

        <Button
            ohos:id="$+id:bt_de"
            ohos:width="60vp"
            ohos:height="60vp"
            ohos:text_size="25fp"
            ohos:top_margin="15vp"
            ohos:left_margin="15vp"
            ohos:right_margin="15vp"
            ohos:bottom_margin="15vp"
            ohos:text_alignment="center"
            ohos:background_element="$graphic:btn"
            ohos:text="-"
            ohos:text_font="serif"
            ohos:right_of="$id:bt9"
            ohos:below="$+id:btC"/>

        <Button
            ohos:id="$+id:bt4"
            ohos:width="60vp"
            ohos:height="60vp"
            ohos:text_size="25fp"
            ohos:top_margin="15vp"
            ohos:left_margin="15vp"
            ohos:right_margin="15vp"
            ohos:bottom_margin="15vp"
            ohos:text_alignment="center"
            ohos:background_element="$graphic:btn"
            ohos:text="4"
            ohos:text_weight="1000"
            ohos:below="$+id:bt7"
            />

        <Button
            ohos:id="$+id:bt5"
            ohos:width="60vp"
            ohos:height="60vp"
            ohos:text_size="25fp"
            ohos:top_margin="15vp"
            ohos:left_margin="15vp"
            ohos:right_margin="15vp"
            ohos:bottom_margin="15vp"
            ohos:text_alignment="center"
            ohos:background_element="$graphic:btn"
            ohos:text="5"
            ohos:text_weight="1000"
            ohos:right_of="$+id:bt4"
            ohos:below="$+id:bt7"
            />

        <Button
            ohos:id="$+id:bt6"
            ohos:width="60vp"
            ohos:height="60vp"
            ohos:text_size="25fp"
            ohos:top_margin="15vp"
            ohos:left_margin="15vp"
            ohos:right_margin="15vp"
            ohos:bottom_margin="15vp"
            ohos:text_alignment="center"
            ohos:background_element="$graphic:btn"
            ohos:text="6"
            ohos:text_weight="1000"
            ohos:right_of="$+id:bt5"
            ohos:below="$+id:bt7"
            />

        <Button
            ohos:id="$+id:bt_in"
            ohos:width="60vp"
            ohos:height="60vp"
            ohos:text_size="25fp"
            ohos:top_margin="15vp"
            ohos:left_margin="15vp"
            ohos:right_margin="15vp"
            ohos:bottom_margin="15vp"
            ohos:text_alignment="center"
            ohos:background_element="$graphic:btn"
            ohos:text="+"
            ohos:text_font="serif"
            ohos:right_of="$id:bt9"
            ohos:below="$+id:bt7"/>

        <Button
            ohos:id="$+id:bt1"
            ohos:width="60vp"
            ohos:height="60vp"
            ohos:text_size="25fp"
            ohos:top_margin="15vp"
            ohos:left_margin="15vp"
            ohos:right_margin="15vp"
            ohos:bottom_margin="15vp"
            ohos:text_alignment="center"
            ohos:background_element="$graphic:btn"
            ohos:text="1"
            ohos:text_weight="1000"
            ohos:below="$+id:bt4"
            />

        <Button
            ohos:id="$+id:bt2"
            ohos:width="60vp"
            ohos:height="60vp"
            ohos:text_size="25fp"
            ohos:top_margin="15vp"
            ohos:left_margin="15vp"
            ohos:right_margin="15vp"
            ohos:bottom_margin="15vp"
            ohos:text_alignment="center"
            ohos:background_element="$graphic:btn"
            ohos:text="2"
            ohos:text_weight="1000"
            ohos:right_of="$+id:bt7"
            ohos:below="$+id:bt4"
            />

        <Button
            ohos:id="$+id:bt3"
            ohos:width="60vp"
            ohos:height="60vp"
            ohos:text_size="25fp"
            ohos:top_margin="15vp"
            ohos:left_margin="15vp"
            ohos:right_margin="15vp"
            ohos:bottom_margin="15vp"
            ohos:text_alignment="center"
            ohos:background_element="$graphic:btn"
            ohos:text="3"
            ohos:text_weight="1000"
            ohos:right_of="$+id:bt8"
            ohos:below="$+id:bt4"
            />

        <Button
            ohos:id="$+id:bt_eq"
            ohos:width="60vp"
            ohos:height="150vp"
            ohos:text_size="25fp"
            ohos:top_margin="15vp"
            ohos:left_margin="15vp"
            ohos:right_margin="15vp"
            ohos:bottom_margin="15vp"
            ohos:text_alignment="center"
            ohos:background_element="$graphic:btn"
            ohos:text="="
            ohos:text_font="serif"
            ohos:right_of="$id:bt9"
            ohos:below="$+id:bt4"/>

        <Button
            ohos:id="$+id:bt_per"
            ohos:width="60vp"
            ohos:height="60vp"
            ohos:text_size="25fp"
            ohos:top_margin="15vp"
            ohos:left_margin="15vp"
            ohos:right_margin="15vp"
            ohos:bottom_margin="15vp"
            ohos:text_alignment="center"
            ohos:background_element="$graphic:btn"
            ohos:text="%"
            ohos:text_weight="1000"
            ohos:below="$+id:bt1"
            />

        <Button
            ohos:id="$+id:bt0"
            ohos:width="60vp"
            ohos:height="60vp"
            ohos:text_size="25fp"
            ohos:top_margin="15vp"
            ohos:left_margin="15vp"
            ohos:right_margin="15vp"
            ohos:bottom_margin="15vp"
            ohos:text_alignment="center"
            ohos:background_element="$graphic:btn"
            ohos:text="0"
            ohos:text_weight="1000"
            ohos:right_of="$+id:bt_per"
            ohos:below="$+id:bt1"
            />

        <Button
            ohos:id="$+id:bt_end"
            ohos:width="60vp"
            ohos:height="60vp"
            ohos:text_size="25fp"
            ohos:top_margin="15vp"
            ohos:left_margin="15vp"
            ohos:right_margin="15vp"
            ohos:bottom_margin="15vp"
            ohos:text_alignment="center"
            ohos:background_element="$graphic:btn"
            ohos:text="."
            ohos:text_weight="1000"
            ohos:right_of="$+id:bt0"
            ohos:below="$+id:bt1"/>

    </DependentLayout>

</DependentLayout>